{
  "name": "jest-test-novacare",
  "version": "0.0.1",
  "description": "Testing the unit test, for science!",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "test-watch": "jest --watchAll"
  },
  "keywords": [
    "jest",
    "js",
    "es6"
  ],
  "author": "karl.solgaard@novacare.no",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "jest-cli": "20.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "\\.test\\.js$"
  }
}
